{
  "name": "bookshelf-sqlite",
  "version": "1.0.0",
  "description": "example app using react, node and sqlite",
  "private": false,
  "license": "MIT",
  "browserlist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "src/index.tsx",
  "prox": "http://localhost:4001",
  "scripts": {
    "build": "node ./builsScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
    "start-server": "nodemon server/server.js --watch server/*",
    "start-front": "react-scripts start",
    "start": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "axios": "0.19.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "helmet": "3.22.0",
    "knex": "0.20.13",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "concurrently": "5.1.0",
    "nodemon": "2.0.3",
    "react-scripts": "3.4.1",
    "sqlite3": "4.1.1",
    "typescript": "3.8.3"
  }
}
