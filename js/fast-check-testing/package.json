{
  "name": "fast-check-testing",
  "version": "1.0.0",
  "description": "Learning Property Based Testing in Javascript, with the Fast Check framework.",
  "main": "main.js",
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/specs/*.+(ts|tsc|js)"
    ]
  },
  "keywords": [
    "property",
    "based",
    "testing",
    "fast",
    "check",
    "javascript",
    "quick",
    "check"
  ],
  "author": "Eduardo Fran√ßa",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "fast-check": "^2.20.0",
    "jest": "^27.4.5",
    "ts-jest": "^27.1.1"
  },
  "dependencies": {
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
